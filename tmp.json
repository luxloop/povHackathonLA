{
  "name": "IvayloGetovDotCom",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true
    },
    "globals": {
      "Modernizr": true,
      "p5": true,
      "Vector2d": true,
      "rot13rot5Encode": true,
      "Cube": true
    },
    "rules": {
      "linebreak-style": [
        2,
        "unix"
      ],
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-console": 1
    }
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0"
  }
}
